{
  "dashboard": {
    "title": "OpenClaw Plugin - Production Monitoring",
    "tags": ["openclaw", "nexus", "production"],
    "timezone": "utc",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "{{method}} {{route}} - {{status_code}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Requests/sec", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Latency (p95)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"nexus-openclaw\"}[5m]))",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Latency (s)", "format": "s" },
          { "show": false }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [1], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High HTTP Latency",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "rate(http_request_errors_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "{{method}} {{route}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Errors/sec", "format": "short" },
          { "show": false }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [0.05], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Error Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Active WebSocket Connections",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "websocket_connections_active{service=\"nexus-openclaw\"}",
            "legendFormat": "Org: {{organization_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Connections", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 5,
        "title": "Active Sessions by Tier",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "sessions_active{service=\"nexus-openclaw\"}",
            "legendFormat": "{{tier}} - {{organization_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Sessions", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 6,
        "title": "Session Creation Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "rate(sessions_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "{{tier}} - {{organization_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Sessions/sec", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 7,
        "title": "Skill Execution Success Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "sum(rate(skill_executions_total{service=\"nexus-openclaw\",status=\"success\"}[5m])) / sum(rate(skill_executions_total{service=\"nexus-openclaw\"}[5m]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Rate", "format": "percentunit", "min": 0, "max": 1 },
          { "show": false }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [0.9], "type": "lt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "5m", "now"] },
              "reducer": { "params": [], "type": "avg" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "Low Skill Success Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 8,
        "title": "Skill Execution Duration (p95)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(skill_execution_duration_seconds_bucket{service=\"nexus-openclaw\"}[5m]))",
            "legendFormat": "{{skill_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Duration (s)", "format": "s" },
          { "show": false }
        ]
      },
      {
        "id": 9,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "CPU Cores", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 10,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
        "targets": [
          {
            "expr": "process_resident_memory_bytes{service=\"nexus-openclaw\"}",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Memory", "format": "bytes" },
          { "show": false }
        ]
      },
      {
        "id": 11,
        "title": "Database Query Latency (p95)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(database_query_duration_seconds_bucket{service=\"nexus-openclaw\"}[5m]))",
            "legendFormat": "{{operation}} - {{table}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Latency (s)", "format": "s" },
          { "show": false }
        ]
      },
      {
        "id": 12,
        "title": "Database Active Connections",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 },
        "targets": [
          {
            "expr": "database_connections_active{service=\"nexus-openclaw\"}",
            "legendFormat": "{{pool}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Connections", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 13,
        "title": "Redis Command Latency (p95)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 48 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(redis_command_duration_seconds_bucket{service=\"nexus-openclaw\"}[5m]))",
            "legendFormat": "{{command}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Latency (s)", "format": "s" },
          { "show": false }
        ]
      },
      {
        "id": 14,
        "title": "Rate Limit Hits",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 48 },
        "targets": [
          {
            "expr": "rate(rate_limit_hits_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "{{tier}} - {{limit_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Hits/sec", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 15,
        "title": "Quota Usage by Tier",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 56 },
        "targets": [
          {
            "expr": "quota_usage{service=\"nexus-openclaw\"} / quota_limit{service=\"nexus-openclaw\"}",
            "legendFormat": "{{tier}} - {{quota_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Usage Ratio", "format": "percentunit", "min": 0, "max": 1 },
          { "show": false }
        ]
      },
      {
        "id": 16,
        "title": "Message Processing Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 56 },
        "targets": [
          {
            "expr": "rate(messages_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "{{tier}} - {{direction}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Messages/sec", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 17,
        "title": "External Service Errors",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 64 },
        "targets": [
          {
            "expr": "rate(external_service_errors_total{service=\"nexus-openclaw\"}[5m])",
            "legendFormat": "{{service}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Errors/sec", "format": "short" },
          { "show": false }
        ]
      },
      {
        "id": 18,
        "title": "Pod Restarts",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 64 },
        "targets": [
          {
            "expr": "kube_pod_container_status_restarts_total{pod=~\"nexus-openclaw.*\"}",
            "legendFormat": "{{pod}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "label": "Restarts", "format": "short" },
          { "show": false }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": { "params": [3], "type": "gt" },
              "operator": { "type": "and" },
              "query": { "params": ["A", "10m", "now"] },
              "reducer": { "params": [], "type": "diff" },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Pod Restart Rate",
          "noDataState": "no_data",
          "notifications": []
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
